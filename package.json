{
  "name": "anu-portfolio",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "predev": "rm -rf .next || true",
    "dev": "next dev -H 127.0.0.1",
    "dev:turbo": "next dev -H 127.0.0.1 --turbopack",
    "dev:stable": "rm -rf .next && NEXT_TURBOPACK=0 next dev -H 127.0.0.1",
    "dev:clean": "rm -rf .next && next dev -H 127.0.0.1",
    "dev:restart": "pkill -f 'next dev' || true; sleep 2; rm -rf .next && npm run dev:stable",
    "clean:all": "rm -rf .next out node_modules/.cache .turbo",
    "build": "next build",
    "prebuild": "rm -rf out node_modules/.cache .turbo || true && node scripts/generate-sitemap.js",
    "build:clean": "npm run clean:all && npm run build",
    "build:fresh": "npm run clean:all && node scripts/generate-sitemap.js && next build",
    "sitemap": "node scripts/generate-sitemap.js",
    "start": "next start",
    "lint": "eslint . --ext .ts,.tsx",
    "clean": "rm -rf .next",
    "clean:full": "bash scripts/clean-build.sh",
    "deploy:s3": "bash scripts/deploy-s3.sh",
    "ship": "npm run build && git add -A && git commit -m 'deploy' && git push origin main && aws s3 sync out s3://anujaharsha.com --delete --region us-east-1 && aws cloudfront create-invalidation --distribution-id E1RKSKYEABLX6E --paths '/*' --region us-east-1"
  },
  "dependencies": {
    "framer-motion": "^11.15.0",
    "lucide-react": "^0.556.0",
    "next": "^16.0.10",
    "react": "^19.2.1",
    "react-dom": "^19.2.1"
  },
  "devDependencies": {
    "@babel/plugin-bugfix-firefox-class-in-computed-class-key": "^7.25.9",
    "@babel/plugin-bugfix-safari-class-field-initializer-scope": "^7.25.9",
    "@babel/plugin-bugfix-safari-id-destructuring-collision-in-function-expression": "^7.25.9",
    "@babel/plugin-bugfix-v8-spread-parameters-in-optional-chaining": "^7.25.9",
    "@babel/plugin-bugfix-v8-static-class-fields-redefine-readonly": "^7.25.9",
    "@types/node": "^22.10.2",
    "@types/react": "^19.0.1",
    "@types/react-dom": "^19.0.1",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.57.1",
    "eslint-config-next": "^15.0.0",
    "postcss": "^8.4.49",
    "sharp": "^0.34.5",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.6.3"
  }
}
