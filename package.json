{
  "name": "anu-portfolio",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "predev": "rm -rf .next || true",
    "dev": "next dev",
    "dev:clean": "rm -rf .next && next dev -H 127.0.0.1",
    "dev:restart": "pkill -f 'next dev' || true; sleep 2; rm -rf .next && next dev -H 127.0.0.1",
    "clean:all": "rm -rf .next out node_modules/.cache .turbo",
    "build": "next build",
    "prebuild": "rm -rf .next out node_modules/.cache .turbo || true && node scripts/generate-sitemap.js",
    "postbuild": "node scripts/generate-resume-pdf.js",
    "build:clean": "npm run clean:all && npm run build",
    "build:fresh": "npm run clean:all && node scripts/generate-sitemap.js && next build && node scripts/generate-resume-pdf.js",
    "sitemap": "node scripts/generate-sitemap.js",
    "start": "next start",
    "lint": "next lint",
    "clean": "rm -rf .next",
    "clean:full": "bash scripts/clean-build.sh",
    "deploy:s3": "bash scripts/deploy-s3.sh"
  },
  "dependencies": {
    "framer-motion": "^11.11.1",
    "html2pdf.js": "^0.12.1",
    "next": "14.2.18",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@babel/plugin-bugfix-firefox-class-in-computed-class-key": "^7.28.5",
    "@babel/plugin-bugfix-safari-class-field-initializer-scope": "^7.27.1",
    "@babel/plugin-bugfix-safari-id-destructuring-collision-in-function-expression": "^7.27.1",
    "@babel/plugin-bugfix-v8-spread-parameters-in-optional-chaining": "^7.27.1",
    "@babel/plugin-bugfix-v8-static-class-fields-redefine-readonly": "^7.28.3",
    "@svgr/webpack": "^8.1.0",
    "@types/node": "^22.10.2",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.57.1",
    "eslint-config-next": "14.2.18",
    "postcss": "^8.4.49",
    "puppeteer": "^24.32.0",
    "react-devtools": "^7.0.1",
    "sharp": "^0.34.5",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.6.3"
  }
}